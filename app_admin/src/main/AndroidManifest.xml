<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mma.noshow_admin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"></activity>
        <activity
            android:name=".AdminHomeActivity"
            android:label="@string/title_activity_admin_home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AdminProfileActivity"
            android:label="@string/title_activity_admin_profile"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".AdminFoodsActivity"
            android:label="@string/title_activity_admin_foods"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>